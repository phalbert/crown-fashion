{
  "name": "crown-fashion",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://phalbert.github.com/crown-attires",
  "dependencies": {
    "firebase": "^7.4.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "docker stop $(docker ps -q --filter ancestor=$npm_package_name:$npm_package_version) && docker rm $npm_package_name",
    "docker": "DOCKER_BUILDKIT=1 docker build -t $npm_package_name:$npm_package_version .",
    "postdocker": "docker run -d -p 3000:3000 -v ${PWD}:/app -v /app/node_modules --name $npm_package_name $npm_package_name:$npm_package_version",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
